{
    "containerDefinitions": [
        {
            "name": "nodejs-service",
            "image": "253921445383.dkr.ecr.ap-southeast-1.amazonaws.com/nodejs-random-color",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 3000,
                    "protocol": "tcp",
                    "containerPort": 3000
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "nodejs-task-definition"
}
